<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
  <context id="jpz" targetRuntime="MyBatis3">
    <jdbcConnection 
    driverClass="com.mysql.jdbc.Driver" 
    connectionURL="jdbc:mysql://192.168.0.101:3306/jpz?zeroDateTimeBehavior=convertToNull&amp;useUnicode=true&amp;characterEncoding=utf-8" 
    userId="root" 
    password="root" />
    <!-- 生成实体对象 -->
    <javaModelGenerator targetPackage="com.aaron.jpz.model" targetProject="\CoreFrame\src">
    	<property name="enableSubPackages" value="true"/>
    	<property name="trimStrings" value="true"/>
    </javaModelGenerator>
    <!-- 生成mapper文件 -->
    <sqlMapGenerator targetPackage="com.aaron.jpz.model" targetProject="\CoreFrame\src">
    	<property name="enableSubPackages" value="true"/>
    </sqlMapGenerator>
    
    <javaClientGenerator targetPackage="com.aaron.jpz.dao" targetProject="\CoreFrame\src" type="XMLMAPPER" />
    <table schema="???" tableName="???" >
      <columnOverride column="???" property="???" />
    </table>
  </context>
</generatorConfiguration>